<h1>Grouping By Category</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>category</th>
            <th>amount</th>
        </tr>
    </thead>
    <tbody>
        <% results.map(item => {%>
            <tr>
               <td> <%= item._id.category %> </td>
               <td><%= item._id.total %></td>
            </tr>
            
            <% }) %>

    </tbody>
</table>
